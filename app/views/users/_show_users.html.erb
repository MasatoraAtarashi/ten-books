<% if @users.any? %>
  <ul class="collection" id="index-collection">

    <% @users.each do |user| %>
      <li class="collection-item avatar">
        <img src="" alt="" class="circle">
        <span class="title"><%= link_to user.name, user %></span>
      </li>
    <% end %>
  </ul>
  <%= will_paginate @users %>
<% end %>
